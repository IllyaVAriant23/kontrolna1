#include <stdio.h>
#include <math.h>

int main() {
    
  double x1, y1, x2, y2;
    double ilop;

    printf("Введіть координати точки x1, y1 : ");
    scanf("%lf %lf", &x1, &y1);

    printf("Введіть координати точки x2 , y2 : ");
    scanf("%lf %lf", &x2, &y2);

    ilop = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    printf("Відстань між точками: %lf\n", ilop);

    return 0;
}
